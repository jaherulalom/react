{
  "name": "Booking-Demo-App",
  "version": "1.0.0",
  "description": "Booking Demo App WebApp",
  "main": "index.js",
  "scripts": {
    "watch": "ENVIRONMENT=local webpack -d --watch",
    "build": "webpack",
    "serve": "node server.js",
    "dev": "concurrently \"yarn watch\" \"yarn serve\"",
    "e2e-server": "node_modules/.bin/webpack-dev-server --port=9090 --quiet",
    "test": "npm run test:lint && npm run test:unit && npm run test:coverage && npm run test:e2e",
    "test:lint": "eslint ./src",
    "test:unit": "ENVIRONMENT=test mocha test/unit --compilers js:babel-register --require ./test/testHelper.js --require babel-polyfill --recursive",
    "test:coverage": "ENVIRONMENT=test nyc mocha test/unit --compilers js:babel-register --require ./test/testHelper.js --recursive",
    "test:e2e": "ENVIRONMENT=test ./node_modules/.bin/nightwatch --config nightwatch.config.js test/feature"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 85,
    "statements": 80,
    "functions": 50,
    "branches": 60,
    "reporter": [
      "lcov",
      "text"
    ]
    },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.1",
    "jsdom": "9.12.0",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.0",
    "nyc": "^10.3.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "axios-mock-adapter": "^1.8.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "express": "^4.14.1",
    "history": "^4.6.1",
    "material-ui": "^0.18.3",
    "nightwatch": "^0.9.15",
    "prop-types": "^15.5.10",
    "rc-slider": "^8.1.3",
    "react": "^15.6.1",
    "react-burger-menu": "^2.1.1",
    "react-dom": "^15.6.1",
    "react-mdl": "^1.10.3",
    "react-rangeslider": "^2.0.1",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-spinkit": "^3.0.0",
    "react-spinner-material": "^1.0.13",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.5.4",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
